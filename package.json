{
  "name": "commander",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "commander": "node index.js --new-version 1.1.0 --file-name index.js",
    "commander:short": "node index.js -nv 1.1.0 -fn index.js",
    "commander:error": "node index.js -e",
    "commander:version": "node index.js --new-version=1.1.0",
    "commander:combined": "node index.js -ev 1.1.22",
    "commander:no-log": "node index.js --no-log",
    "commander:no-color": "node index.js",
    "commander:required-color": "node index.js --color",
    "commander:red-color": "node index.js --color=red",
    "commander:required": "node index.js -f",
    "commander:letters": "node index.js --letters=a,s,d,e --color=black",
    "commander:another-version": "node index.js -v",
    "commander:help": "node index.js --help",
    "commander:timeout": "node index.js --timeout 120"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Abdilar/commander.git"
  },
  "keywords": [
    "commander",
    "args",
    "npm",
    "arguments"
  ],
  "author": "Saeed Abdilar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Abdilar/commander/issues"
  },
  "homepage": "https://github.com/Abdilar/commander#readme",
  "dependencies": {
    "commander": "^12.0.0"
  }
}
